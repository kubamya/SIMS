<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.proxy.mapper.ProxyMapper">
    <select id="getCityCode" parameterType="java.util.Map" resultType="com.example.model.City">
		select c_code as CCode, c_name as  CName from t_common_city
        where c_name = #{city, jdbcType=VARCHAR}
	</select>

</mapper>
